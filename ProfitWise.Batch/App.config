<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  
  <appSettings>
    <!-- Shopify Configuration -->
    <add key="shopify_config_apikey" value="50d69dbaf54ee35929a946790d5884e4" />
    <add key="shopify_config_apisecret" value="eb52fb7d5612e383d2e9513001a012eb" />
    
    <!-- Encryption Service Keys -->
    <add key="security_aes_key" value="12345678901234gj12345678901234gj" />
    <add key="security_aes_iv" value="1234567890123456" />

    <!-- Shopify Http Client -->
    <add key="ShopifyRetryLimit" value="3" />
    <add key="ShopifyHttpTimeout" value="60000" />
    <add key="ShopifyThrottlingDelay" value="500" />
    <add key="ShopifyRetriesEnabled" value="true" />

    <!-- TODO - query the MySQL server for its zone -->
    <add key="Machine_TimeZone" value="(GMT-06:00) Central Time (US &amp; Canada)" />
    
    <!-- Fixer API Http Client -->
    <add key="FixerApiRetryLimit" value="3" />
    <add key="FixerApiHttpTimeout" value="60000" />
    <add key="FixerApiThrottlingDelay" value="0" />
    <add key="FixerApiRetriesEnabled" value="true" />
    
    <!-- Refresh Service Settings -->
    <add key="RefreshServiceMaxOrderRate" value="250" />
    <add key="RefreshServiceMaxProductRate" value="100" />
    <add key="ShopRefreshInterval" value="* * * * *" />
    <add key="ExchangeRefreshInterval" value="* * * * *" />
    
    <!-- ProfitWise.Data Diagnostic shim -->
    <add key="ProfitWiseDiagonosticShop" value="955973" />
    <add key="ProfitWiseDiagonosticOrders" value="" />
    
  </appSettings>
  
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>      
      <target name="Standard.Output" xsi:type="SplitGroup">
        <target xsi:type="File" filename="C:\DEV\ProfitWise\Logs\${logger}.${shortdate}.txt" layout="${longdate}|${level}|${message}" archiveAboveSize="50000000"></target>
        <target xsi:type="Console" layout="${message} ${onexception:|${exception:format=tostring} | ${stacktrace}}"></target>
        <target xsi:type="Debugger" layout="${message} ${onexception:|${exception:format=tostring} | ${stacktrace}}"></target>
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="INFO" writeto="Standard.Output"></logger>
    </rules>
  </nlog>  
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <connectionStrings>
    <!--<add name="DefaultConnection" providerName="MySql.Data.MySqlClient" connectionString="Persist Security Info=True;server=localhost;uid=root;pwd=sqlBoomba123!@#;database=profitwise;" />-->
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Server=localhost;Database=ProfitWise;Trusted_Connection=True;" />
  </connectionStrings>
  
  <runtime>
