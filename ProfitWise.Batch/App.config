<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="profitWiseConfiguration" type="System.Configuration.SingleTagSectionHandler" />
  </configSections>
  
  <appSettings>
    <!-- Shopify Http Client -->
    <add key="ShopifyRetryLimit" value="3" />
    <add key="ShopifyHttpTimeout" value="60000" />
    <add key="ShopifyThrottlingDelay" value="500" />
    <add key="ShopifyRetriesEnabled" value="true" />
    
    <!-- Machine Time Zone -->
    <add key="Machine_TimeZone" value="(GMT-06:00) Central Time (US &amp; Canada)" />
    
    <!-- Fixer Exchange Rate API Http Client -->
    <add key="FixerApiRetryLimit" value="3" />
    <add key="FixerApiHttpTimeout" value="60000" />
    <add key="FixerApiThrottlingDelay" value="0" />
    <add key="FixerApiRetriesEnabled" value="true" />
    
    <!-- HangFire execution frequencies -->
    <add key="ShopRefreshInterval" value="* * * * *" />
    <add key="ExchangeRefreshInterval" value="* * * * *" />
    <add key="CleanupServiceInterval" value="* * * * *" />

    <!-- Batch Process settings -->
    <add key="InitialOrderStartDateOffsetMonths" value="3" />
    <add key="RefreshServiceMaxOrderRate" value="250" />
    <add key="RefreshServiceMaxProductRate" value="100" />
    <add key="PickListExpirationSeconds" value="60" />
    <add key="ReportDataExpirationSeconds" value="60" />
    
    <!-- ProfitWise.Data Diagnostic shim -->
    <add key="ProfitWiseDiagonosticShop" value="955973" />
    <add key="ProfitWiseDiagonosticOrders" value="" />    
  </appSettings>

  <profitWiseConfiguration
      ShopifyApiKey=""
      ShopifyApiSecret=""
      ClaimKey=""
      ClaimIv=""
      />

  <!-- NOTE due to inconsistent behavior, Config Transforms are not used on this -->
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>      
      <target name="Standard.Output" xsi:type="SplitGroup">
        <target xsi:type="File" filename="C:\ProfitWise\BatchAppLogs\${logger}.${shortdate}.txt" layout="${longdate}|${level}|${message}" archiveAboveSize="50000000"></target>
        <target xsi:type="Console" layout="${message} ${onexception:|${exception:format=tostring} | ${stacktrace}}"></target>
        <target xsi:type="Debugger" layout="${message} ${onexception:|${exception:format=tostring} | ${stacktrace}}"></target>
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="INFO" writeto="Standard.Output"></logger>
    </rules>
  </nlog>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <connectionStrings>
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Server=localhost;Database=ProfitWise;Trusted_Connection=True;" />
  </connectionStrings>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="linq2db" publicKeyToken="f19f8aed7feff67e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.7.1" newVersion="1.0.7.1" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security.OAuth" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security.Cookies" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.5.0.0" newVersion="3.5.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  
  <system.data>
    <DbProviderFactories>
      <add name="SqlClient Data Provider" invariant="System.Data.SqlClient" description=".Net Framework Data Provider for SqlServer" type="System.Data.SqlClient.SqlClientFactory, System.Data,       Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
    </DbProviderFactories>    
  </system.data>
</configuration>
