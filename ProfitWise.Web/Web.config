<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="profitWiseConfiguration" type="System.Configuration.SingleTagSectionHandler" />
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="aspnet:MaxJsonDeserializerMembers" value="50000" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- How far back to fetch Orders; for DEV we start small to provide "space" for testing -->
    <add key="InitialOrderStartDateOffsetMonths" value="3" />
    <!-- Machine Time Zone -->
    <add key="Machine_TimeZone" value="Eastern Standard Time" />
    <!-- Test Recurring Charges-->
    <add key="TestRecurringCharges" value="true" />
    <!-- General Configuration -->
    <add key="application_root_url" value="https://profitwisedev.com/ProfitWise" />
    <add key="CookieDomain" value="gracie2" />
    <add key="UninstallWebHookAddress" value="https://requestb.in/1kfy04h1" />
    <!-- HangFire execution frequencies -->
    <add key="ShopRefreshInterval" value="*/15 * * * *" />
    <add key="ExchangeRefreshInterval" value="0 2 * * *" />
    <add key="CleanupServiceInterval" value="*/15 * * * *" />
    <!-- File Upload location -->
    <add key="FileUploadDirectory" value="C:\DEV\ProfitWise\TestFileUploads\" />
    <!-- Login -->
    <add key="ProfitWiseLogin" value="/ShopifyAuth/Login" />
  </appSettings>
  <!-- Aleks' XPS15 Win 10 -->
  <profitWiseConfiguration ShopifyApiKey="" ShopifyApiSecret="" ClaimKey="" ClaimIv="" />
  <!-- Aleks' AIR Keys
  <profitWiseConfiguration 
      ShopifyApiKey="" 
      ShopifyApiSecret="" 
      ClaimKey="" 
      ClaimIv="" 
      /> -->
  <connectionStrings>
    <add name="DefaultConnection" providerName="System.Data.SqlClient" connectionString="Server=localhost;Database=ProfitWise;Trusted_Connection=True;" />
  </connectionStrings>
  <nlog>
    <targets>
      <target name="Standard.Output" type="SplitGroup">
        <target type="File" filename="C:\DEV\ProfitWise\Logs\${logger}.${shortdate}.txt" layout="${longdate}|${level}|${message}" archiveAboveSize="50000000" />
        <target type="Console" layout="${message} ${exception:format=tostring}" />
        <target type="Debugger" layout="${message} ${exception:format=tostring}" />
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="INFO" writeto="Standard.Output" />
    </rules>
  </nlog>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.7.2" />
      </system.Web>
  -->
  <system.web>
    <authentication mode="None" />
    <!--<httpRuntime maxRequestLength="10240" />-->
    <authorization>
      <deny users="*" verbs="OPTIONS, PROPFIND, HEAD" />
    </authorization>
    <compilation debug="true" targetFramework="4.7.2" />
    <httpRuntime targetFramework="4.5.2" />
    <httpModules />
    <customErrors mode="RemoteOnly" defaultRedirect="~/Error/Http500" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
    <staticContent>
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="font/woff2" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <runtime>
